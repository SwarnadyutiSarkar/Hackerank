
class TestDataEmptyArray:
    @staticmethod
    def get_array():
        return []

class TestDataUniqueValues:
    @staticmethod
    def get_array():
        return [5, 2, 8, 1, 9]

    @staticmethod
    def get_expected_result():
        return 3

class TestDataExactlyTwoDifferentMinimums:
    @staticmethod
    def get_array():
        return [5, 1, 1, 8, 9]

    @staticmethod
    def get_expected_result():
        return 1
